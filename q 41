#include <stdio.h>
#include <math.h>

int main() {
    int num, firstDigit, lastDigit, digits, swappedNum;
    
    // Input the number
    printf("Enter a positive integer: ");
    scanf("%d", &num);

    lastDigit = num % 10; // Extract last digit

    // Count number of digits and extract first digit
    digits = (int)log10(num);       // Number of digits minus 1
    firstDigit = num / (int)pow(10, digits); // Extract first digit

    // Remove first and last digit from number
    int middlePart = num % (int)pow(10, digits); // Remove first digit
    middlePart = middlePart / 10;                // Remove last digit

    // Construct new number after swapping
    swappedNum = lastDigit * (int)pow(10, digits) + middlePart * 10 + firstDigit;

    printf("Number after swapping first and last digits: %d\n", swappedNum);

    return 0;
}
