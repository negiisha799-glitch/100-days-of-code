#include <stdio.h>

int main() {
    int num, remainder, product = 1;
    int hasOddDigit = 0;  

    
    printf("Enter a positive integer: ");
    scanf("%d", &num);

    int originalNum = num;

    
    while (num != 0) {
        remainder = num % 10; 

        if (remainder % 2 != 0) { 
            product *= remainder;
            hasOddDigit = 1;
        }

        num /= 10; 
    }

    if (hasOddDigit)
        printf("The product of odd digits of %d is %d\n", originalNum, product);
    else
        printf("There are no odd digits in %d\n", originalNum);

    return 0;
}
