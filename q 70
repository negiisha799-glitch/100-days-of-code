#include <stdio.h>

int main() {
    int arr[100], n, k, i;
    int temp[100];

    // Input the number of elements
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    // Input array elements
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Input k (number of positions to rotate)
    printf("Enter the number of positions to rotate to the right: ");
    scanf("%d", &k);

    // Adjust k if it is greater than n
    k = k % n;

    // Copy last k elements to the beginning of temp
    for(i = 0; i < k; i++) {
        temp[i] = arr[n - k + i];
    }

    // Copy the first n - k elements after that
    for(i = 0; i < n - k; i++) {
        temp[k + i] = arr[i];
    }

    // Copy temp back to arr
    for(i = 0; i < n; i++) {
        arr[i] = temp[i];
    }

    // Print the rotated array
    printf("Array after rotating to the right by %d positions:\n", k);
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
