#include <stdio.h>

int main() {
    int num, sum = 0, remainder;

    
    printf("Enter a positive integer: ");
    scanf("%d", &num);

    int originalNum = num;

    
    while (num != 0) {
        remainder = num % 10;  
        sum += remainder;      
        num /= 10;            
    }

    printf("The sum of digits of %d is %d\n", originalNum, sum);

    return 0;
}
