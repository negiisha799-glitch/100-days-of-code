#include <stdio.h>

int main() {
    int n, i, j, isPrime;

    printf("Enter the value of N: ");
    scanf("%d", &n);

    printf("Prime numbers from 1 to %d are:\n", n);

    for(i = 2; i <= n; i++) {   // Start from 2 because 1 is not prime
        isPrime = 1;           // Assume the number is prime

        for(j = 2; j <= i / 2; j++) {
            if(i % j == 0) {   // If divisible, it's not prime
                isPrime = 0;
                break;
            }
        }

        if(isPrime == 1) {
            printf("%d ", i);
        }
    }

    return 0;
}
