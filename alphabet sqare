#include <stdio.h>

int main() {
    int n, i, j;
    printf("Enter the size of alphabet square: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            printf("%c ", 'A' + j);  // print alphabets from A onwards
        }
        printf("\n");
    }

    return 0;
}
