#include <stdio.h>


int findCeilIndex(int arr[], int n, int x) {
    int start = 0, end = n - 1;
    int result = -1;

    while (start <= end) {
        int mid = (start + end) / 2;

        if (arr[mid] >= x) {
            result = mid;      
            end = mid - 1;     
        } else {
            start = mid + 1;   
        }
    }

    return result;
}

int main() {
    int n, x;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter elements (sorted): ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter x: ");
    scanf("%d", &x);

    int ceilIndex = findCeilIndex(arr, n, x);

    if (ceilIndex == -1) {
        printf("-1\n");
    } else {
        printf("Ceil index: %d\n", ceilIndex);
    }

    return 0;
}

