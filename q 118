#include <stdio.h>

int main() {
    int n, i, sum = 0, total;
    
    printf("Enter the value of n: ");
    scanf("%d", &n);
    
    int arr[n];
    
    printf("Enter %d numbers (from 0 to %d, with one missing):\n", n, n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    
    total = n * (n + 1) / 2;  
    printf("Missing number is: %d\n", total - sum);
    
    return 0;
}

