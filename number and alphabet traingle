#include <stdio.h>

int main() {
    int n, i, j;
    printf("Enter number of rows: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        if (i % 2 != 0) {
            // Odd row → print numbers
            for (j = 1; j <= i; j++) {
                printf("%d ", j);
            }
        } else {
            // Even row → print alphabets
            for (j = 0; j < i; j++) {
                printf("%c ", 'A' + j);
            }
        }
        printf("\n");
    }

    return 0;
}
