#include <stdio.h>

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    // print a pattern of stars
    for (int i = 1; i <= 3; i++) { //outer loop signify no.of lines
        for (int j = 1; j <= n; j++) { // inner loop signify no.of stars in each line
            printf("*");
        }
        printf("\n");  // go to next line after printing stars
    }

    return 0;
}
