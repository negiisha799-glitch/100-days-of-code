#include <stdio.h>

int main() {
    int n, k;
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter the array elements: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter the subarray size k: ");
    scanf("%d", &k);

    if (k > n) {
        printf("k cannot be greater than array size.\n");
        return 0;
    }

    printf("First negative elements in each subarray of size %d: ", k);

    // Traverse all windows
    for (int i = 0; i <= n - k; i++) {
        int firstNegative = 0;  // 0 if no negative found
        for (int j = 0; j < k; j++) {
            if (arr[i + j] < 0) {
                firstNegative = arr[i + j];
                break;  // first negative found
            }
        }
        printf("%d ", firstNegative);
    }

    printf("\n");
    return 0;
}
